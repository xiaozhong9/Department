<script src="${root}/js/layui.js" charset="utf-8"></script>
<script type="text/javascript">
		var vm=new Vue({
			el:"#column",
			data:{
				page:{
					columnID:${columnID},
					currentPage:1,		//当前页
					pageSize:7
				},
				result:{
					size:0,//当前页的数量
					navigatepageNums:[],//所有导航页号
					list:[
						{
							aid:0,
							title:null,
							pubTime:null
						}
					],//文章
					isFirstPage:null,//是否为第一页
					isLastPage:null,//是否为最后一页
					total:0,//一共多少条数据
					pageNum:1//当前页
				}
			},
			methods:{
				 findArticleByPage:function(){	
					this.$http.get('${root}/column/get/column/page/'+this.page.columnID+"/"+vm.page.currentPage+"/"+this.page.pageSize,{
					},{emulateJSON:true})
					.then(function(res){
						this.result=res.body;
						console.log(this.result);
						})
				},
				 selectPage:function(curr){
					 this.page.currentPage=curr.target.value;
					 this.findArticleByPage();
					 layui.use(['laypage', 'layer']);
					 var laypage=layui.laypage;
					 laypage.render({
						    elem: 'demo7'
						    ,count: vm.result.total
						    ,curr:vm.page.currentPage
						    ,limit:vm.page.pageSize
						    ,layout: [ 'prev', 'page', 'next', 'refresh', 'skip']
						    ,jump: function(obj,first){
						    	if(!first){
						    	vm.page.currentPage=obj.curr;
						    	vm.findArticleByPage();
						    	}
						    }
						  });
			}
				
			}
		})
		window.onload=function(){
			vm.findArticleByPage();
			setTimeout('page()', 100); 
		}
		function page(){
			
			 var obj=layui.use(['laypage', 'layer'], function(){
			  var laypage = layui.laypage
			  ,layer = layui.layer;
			  laypage.render({
			    elem: 'demo7'
			    ,curr:vm.page.currentPage
			    ,count: vm.result.total
			    ,limit:vm.page.pageSize
			    ,layout: [ 'prev', 'page', 'next', 'refresh', 'skip']
			    ,jump: function(obj,first){
			    	//首次不执行
			    	if(!first){
			    	vm.page.currentPage=obj.curr;
			    	vm.findArticleByPage();
			    	}
			    	
			    }
			  });
			  //将一段数组分页展示
			});
		}
		
		
</script>
