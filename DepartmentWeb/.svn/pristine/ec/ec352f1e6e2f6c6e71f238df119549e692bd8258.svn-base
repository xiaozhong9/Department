}
.ih-item.circle.effect1 .img {
	position: absolute;
	top: 10px;
	bottom: 0;
	left: 10px;
	right: 0;
	width: auto;
	height: auto
}
.ih-item.circle.effect1 .img:before {
	display: none
}
.ih-item.circle.effect1.colored .info {
	background: #1a4a72;
	background: rgba(26,74,114,.6)
}
.ih-item.circle.effect1 .info {
	top: 10px;
	bottom: 0;
	left: 10px;
	right: 0;
